{
    "src": [{
        "haruna": "./src/*.js",
        "start": "./README.md"
    }],
    "dest": "./docs",
    "base": "./docs",
    "clean": true,
    "jsdoc": {
        "plugins": ["plugins/markdown"]
    },
    "app": {
        "title": "Haruna Documentation",
        "entrance": "content:start"
    }
}