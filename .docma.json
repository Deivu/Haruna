{
    "src": [
        "./src/*.js",
        "./README.md"
    ],
    "dest": "./docs",
    "clean": true,
    "jsdoc": {
        "plugins": ["plugins/markdown"]
    },
    "markdown": {
        "sanitize": false
    },
    "app": {
        "title": "Haruna Documentation",
        "entrance": "content:readme",
        "base": "/Haruna",
        "routing": {
            "method": "path"
        }
    }
}